<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.ltd.uk/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create creator="spring" javascript="securityDWRService">
			<param name="beanName" value="securityDWRService" />
		</create>

		<convert converter="bean" match="com.resoft.prophet.widgets.extjs.data.TreeNode" />
	</allow>
	<signatures>
		<![CDATA[
			import java.util.Map;
			import com.resoft.prophet.widgets.extjs.data.TreeNode;
			
			securityDWRService.getMenuTreeNodes(TreeNode, Map<String, String>);
		]]>
	</signatures>
</dwr>
